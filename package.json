{
  "name": "@taro-uno/ui",
  "version": "0.9.0-beta",
  "description": "Simplified multi-platform UI component library for Taro ecosystem",
  "keywords": [
    "taro",
    "react",
    "ui",
    "components",
    "typescript",
    "mobile",
    "mini-program"
  ],
  "type": "module",
  "main": "./dist/taro-uno-ui.cjs.js",
  "module": "./dist/taro-uno-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/taro-uno-ui.es.js",
      "require": "./dist/taro-uno-ui.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --mode lib && npm run types:build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:debug": "vitest run --coverage --reporter=verbose",
    "test:watch": "vitest watch",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist .vite coverage",
    "tokens:generate": "tsx scripts/generate-scss-tokens.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "precommit": "npm run type-check && npm run lint:check && npm run test:run",
    "prebuild": "npm run clean && npm run type-check",
    "types:build": "tsc -p tsconfig.build.json",
    "docs:generate": "typedoc",
    "docs:watch": "typedoc --watch"
  },
  "dependencies": {
    "@tarojs/components": "^4.1.6",
    "@tarojs/taro": ">=4.1.6",
    "react": ">=19.1.1",
    "react-dom": ">=19.1.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/builder-vite": "^10.0.7",
    "@storybook/cli": "^10.0.7",
    "@storybook/react": "^10.0.7",
    "@storybook/testing-library": "^0.2.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.8",
    "globals": "^16.5.0",
    "jsdom": "^24.0.0",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.6.2",
    "tsx": "^4.19.2",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vitest": "^1.6.1"
  },
  "peerDependencies": {
    "@tarojs/taro": ">=4.1.6",
    "react": ">=19.1.1",
    "react-dom": ">=19.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": {
    "name": "Taro-Uno Team",
    "email": "team@taro-uno.com",
    "url": "https://taro-uno.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agions/taro-uno.git",
    "directory": "packages/ui"
  },
  "homepage": "https://taro-uno.com",
  "bugs": {
    "url": "https://github.com/agions/taro-uno/issues"
  }
}
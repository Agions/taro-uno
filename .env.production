# 生产环境配置
VITE_APP_TITLE=Taro Uno UI
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Taro 多端 UI 组件库

# API 配置
VITE_API_BASE_URL=https://api.taro-uno.com
VITE_API_TIMEOUT=10000

# 构建配置
VITE_BUILD_COMPRESS=true
VITE_BUILD_COMPRESS_TYPE=gzip
VITE_BUILD_ANALYZE=false

# 开发配置
VITE_DEV_PORT=3000
VITE_DEV_HOST=localhost
VITE_DEV_HTTPS=false

# 多端配置
VITE_PLATFORM=h5
VITE_ENABLE_VCONSOLE=false

# 功能开关
VITE_ENABLE_MOCK=false
VITE_ENABLE_PWA=true
VITE_ENABLE_TAILWIND=true

# 第三方服务
VITE_SENTRY_DSN=https://your-sentry-dsn
VITE_GA_TRACKING_ID=GA-XXXXXXXXXX
VITE_ALGOLIA_APP_ID=your-app-id
VITE_ALGOLIA_API_KEY=your-api-key

# CI/CD
VITE_CI=true
VITE_TEST=false